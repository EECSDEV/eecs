<ul>

<% @feedbacks.each do |feedback| %>
        <% if @user.rank >= 3 || @user == feedback.user %>
	<li>
	  <%= feedback.year %>
          <%= feedback.user.last_name %>
          <%= feedback.user.first_name %>
	  <%= link_to 'show', course_feedback_path(@course, feedback) %>
          <% if @user == feedback.user %>
	    <%= link_to 'edit', edit_course_feedback_path(@course, feedback) %>
	    <%= link_to 'destroy', course_feedback_path(@course, feedback),
	                              :method => :delete %>
          <% end %>
	</li>
<% end %>
<% end %>

</ul>

	  <%= link_to 'new feedback', new_course_feedback_path(@course) %>
 <%= link_to 'back to course', course_path(@course)%>
